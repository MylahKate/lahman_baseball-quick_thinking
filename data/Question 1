SELECT yearid, lgid, teamid, franchid, divid, rank, g, ghome, w, l, divwin, wcwin, lgwin, wswin, r, ab, h, h2b, h3b, hr, bb, so, sb, cs, hbp, sf, ra, er, era, cg, sho, sv, ipouts, ha, hra, bba, soa, e, dp, fp, name, park, attendance, bpf, ppf, teamidbr, teamidlahman45, teamidretro
	FROM public.teams;
	

--#1 What range of years for baseball games played does thie provided database cover? 1871 - 2016
SELECT max(yearid)
      ,min(yearid)
FROM teams;

